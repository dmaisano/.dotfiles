# ? git
abbr --add --set-cursor gsl "git stash list"
abbr --add --set-cursor gsm 'git stash -m "%"'

# ? github cli
abbr --add --set-cursor copilot "gh copilot %"
abbr --add --set-cursor gcs "gh copilot suggest %"
abbr --add --set-cursor gce "gh copilot explain %"

# ? node
abbr --add --set-cursor ts_script "npx ts-node -r dotenv/config %"
