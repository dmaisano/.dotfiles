[user]
name = name
email = email
[credential]
helper = store --file ~/.my-credentials
[core]
editor = vim
shell = /opt/homebrew/bin/fish
[init]
defaultBranch = main
[pull]
rebase = false
[push]
autoSetupRemote = true
[merge]
tool = meld
[diff]
tool = meld
[difftool]
prompt = false
[mergetool]
prompt = false
[mergetool "meld"]
trustExitCode = true
